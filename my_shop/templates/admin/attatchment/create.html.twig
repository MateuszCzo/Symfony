{% extends 'base.html.twig' %}

{% block title %}Attatchment Controller{% endblock %}

{% block body %}
    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div class="alert alert-{{ label }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}

    <h1>Attatchment Create</h1>

    {{ form_errors(attatchmentCreateForm) }}

    {{ form_start(attatchmentCreateForm) }}
        {{ form_widget(attatchmentCreateForm) }}

        <button type="submit" class="btn">Create</button>
    {{ form_end(attatchmentCreateForm) }}
{% endblock %}
